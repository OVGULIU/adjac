   function div_qq(x, y) result(z)
    implicit none
    type(adjac_complexan), intent(in) :: x, y
    type(adjac_complexan) :: z
    z%value = x%value / y%value
    z%n = x%n + y%n
    call alloc_mem_q(z%n, z%j)
    call sum_taylor(1d0/y%value, -x%value/(y%value**2), x, y, z)
  end function div_qq

