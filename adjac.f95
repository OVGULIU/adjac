    x%im = 0d0
  end subroutine assign_ba

  !!
  !! operator(+)
  !!

  ! X + Y = x + y + (x_j + y_j) dj

   function add_aa(x, y) result(z)
    implicit none
    type(adjac_double), intent(in) :: x, y
    type(adjac_double) :: z

    z%value = x%value + y%value
    z%n = x%n + y%n
    call alloc_mem_a(z%n, z%j)
    call sum_taylor(dble(1d0), dble(1d0), x, y, z)
  end function add_aa

   function add_ai(x, y) result(z)
    implicit none
    type(adjac_double), intent(in) :: x
    integer, intent(in) :: y
    type(adjac_double) :: z
    z%value = x%value + y
    z%j = x%j
    z%n = x%n
    z%vmul = x%vmul
  end function add_ai

   function add_ia(x, y) result(z)
    implicit none
    integer, intent(in) :: x
    type(adjac_double), intent(in) :: y
    type(adjac_double) :: z
    z = y + x
  end function add_ia
   function add_ad(x, y) result(z)
    implicit none
    type(adjac_double), intent(in) :: x
    double precision, intent(in) :: y
    type(adjac_double) :: z
    z%value = x%value + y
    z%j = x%j
    z%n = x%n
    z%vmul = x%vmul
  end function add_ad

   function add_da(x, y) result(z)
