   function sub_bd(x, y) result(z)
    implicit none
    type(adjac_complex), intent(in) :: x
    double precision, intent(in) :: y
    type(adjac_complex) :: z
    z = x - dcmplx(y)
  end function sub_bd

   function sub_db(x, y) result(z)
    implicit none
    double precision, intent(in) :: x
    type(adjac_complex), intent(in) :: y
    type(adjac_complex) :: z
    z = dcmplx(x) - y
  end function sub_db

