  end function exp_q


  !!
  !! sin
  !!

   function sin_q(x) result(z)
    implicit none
    type(adjac_complexan), intent(in) :: x
    type(adjac_complexan) :: z
    double complex :: v, dv
    v = sin(x%value)
    dv = cos(x%value)
    z = dv*x
    z%value = v
  end function sin_q


  !!
  !! cos
  !!

   function cos_q(x) result(z)
    implicit none
    type(adjac_complexan), intent(in) :: x
    type(adjac_complexan) :: z
    double complex :: v, dv
    v = cos(x%value)
    dv = -sin(x%value)
    z = dv*x
    z%value = v
  end function cos_q


