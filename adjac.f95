   function sub_ab(x, y) result(z)
    implicit none
    type(adjac_double), intent(in) :: x
    type(adjac_complex), intent(in) :: y
    type(adjac_complex) :: z
    z%re = x - y%re
    z%im = -y%im
  end function sub_ab

   function sub_bi(x, y) result(z)
    implicit none
    type(adjac_complex), intent(in) :: x
    integer, intent(in) :: y
    type(adjac_complex) :: z
    z = x - dcmplx(y)
  end function sub_bi

   function sub_ib(x, y) result(z)
    implicit none
    integer, intent(in) :: x
    type(adjac_complex), intent(in) :: y
    type(adjac_complex) :: z
    z = dcmplx(x) - y
