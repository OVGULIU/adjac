    implicit none
    type({{TYPE}}), intent(in) :: x
    double complex, intent(in) :: y
    type(adjac_complex) :: z
    z%re = x * dble(y)
    z%im = x * aimag(y)
  end function mul_az

  {{pure_elemental}} function mul_za(x, y) result(z)
    implicit none
    double complex, intent(in) :: x
    type({{TYPE}}), intent(in) :: y
