    implicit none
    type(adjac_complex), intent(out) :: x
    double complex, intent(in) :: y
    x%re = dble(y)
    x%im = aimag(y)
  end subroutine assign_bz

  {{pure_elemental}} subroutine assign_ba(x, y)
    implicit none
    type(adjac_complex), intent(out) :: x
    type({{TYPE}}), intent(in) :: y
