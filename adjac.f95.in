  {{pure_elemental}} function sub_{{CHR}}{{CHR}}(x, y) result(z)
    implicit none
    type({{TYPE}}), intent(in) :: x, y
    type({{TYPE}}) :: z

    integer :: n

    z%value = x%value - y%value
    z%n = x%n + y%n
    call alloc_mem_{{CHR}}(z%n, z%j)
    call sum_taylor({{CAST}}(1d0), {{CAST}}(-1d0), x, y, z)
